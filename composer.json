{
    "name": "uafrica/oauth-server",
    "description": "OAuth Server for CakePHP 3 using the PHP League's OAuth2 Server",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "oauth", "oauth2", "oauth server", "oauth2 server"],
    "require": {
        "php": ">=7.3.0",
        "cakephp/cakephp": "3.8.*",
        "league/oauth2-server": "~4.1.7",
        "cakephp/migrations": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0",
        "cakephp/cakephp-codesniffer": "^2.3"
    },
    "autoload": {
      "psr-4": {
        "OAuthServer\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
        "OAuthServer\\Test\\": "tests"
      }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Walther Lalk",
            "email": "walther@uafrica.com"
        }
    ]
//    "repositories": [
